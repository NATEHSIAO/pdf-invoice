# API 設定
PORT=8000
HOST=0.0.0.0

# CORS 設定
ALLOWED_ORIGINS=http://localhost:3000

# 安全設定
SECRET_KEY=your-backend-secret-key
ALGORITHM=HS256

# 開發環境設定
DEBUG=1

# Docker 設定
PYTHONPATH=/app
PYTHONUNBUFFERED=1

# 資料庫設定（如果需要）
DATABASE_URL=your-database-url

# 其他服務設定
EMAIL_SERVICE_API_KEY=your-email-service-api-key
PDF_SERVICE_API_KEY=your-pdf-service-api-key 